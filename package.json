{
  "name": "ngs-base",
  "version": "1.0.0",
  "private": true,
  "description": "An example CRUD app with user authentication.",
  "main": "index.js",
  "scripts": {
    "backend": "cd backend && npm run dev",
    "backend:install": "cd backend && npm install",
    "frontend": "cd frontend && npm run dev",
    "frontend:install": "cd frontend && npm install",
    "now-dev": "now dev -p 8008",
    "app": "concurrently -c red,green --kill-others-on-fail \"npm run backend\" \"npm run frontend\""
  },
